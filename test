Sub ProcessAndMoveData()
    Dim ws As Worksheet
    Dim lastRow As Long
    Dim i As Long
    Dim cellValue As String
    
    ' Change the sheet name if necessary
    Set ws = ThisWorkbook.Sheets("Sheet1")
    
    ' Find the last row with data in column D
    lastRow = ws.Cells(ws.Rows.Count, "D").End(xlUp).Row
    
    ' Loop through each row in column D
    For i = 1 To lastRow
        cellValue = ws.Cells(i, 4).Value ' Read the value from column D
        If Left(CStr(Trim(cellValue)), 8) = "- name: " Then
            ' Remove the prefix "- name: " if it exists
            cellValue = Mid(cellValue, 9)
        End If
        ws.Cells(i, 1).Value = cellValue ' Write the modified value to column A
    Next i
    
    ' Display a message to indicate the macro has finished
    MsgBox "Macro has finished running."
End Sub