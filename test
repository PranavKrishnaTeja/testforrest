Sub RemoveLeadingSpaces()
    Dim ws As Worksheet
    Set ws = ThisWorkbook.Sheets("Sheet1") ' Change to your sheet name
    
    Dim lastRowG As Long
    Dim i As Long
    
    ' Find the last row in column G
    lastRowG = ws.Cells(ws.Rows.Count, "G").End(xlUp).Row
    
    ' Debug point to confirm the macro starts
    Debug.Print "Starting RemoveLeadingSpaces macro"
    Debug.Print "Last row in G: " & lastRowG
    
    For i = 1 To lastRowG
        If Len(ws.Cells(i, "G").Value) >= 4 Then
            ws.Cells(i, "G").Value = Mid(ws.Cells(i, "G").Value, 5)
        End If
        ' Debug point to confirm spaces removed
        Debug.Print "Row " & i & " updated: " & ws.Cells(i, "G").Value
    Next i
    
    ' Debug point to confirm the macro ends
    Debug.Print "RemoveLeadingSpaces macro completed"
End Sub